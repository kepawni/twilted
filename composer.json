{
    "name": "kepawni/twilted",
    "version": "v1.2.2",
    "description": "A strictly typed PHP 7 CQRS/ES library/framework suited for PhpSpec and striving for highest IDE intellisense comfort",
    "keywords": [
        "CQRS",
        "event sourcing",
        "PhpSpec"
    ],
    "type": "library",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Kepawni\\Twilted\\": "src/"
        }
    },
    "require-dev": {
        "ext-xdebug": "*",
        "phpspec/phpspec": "^6.1"
    },
    "require": {
        "beberlei/assert": "^3.2",
        "mathiasverraes/classfunctions": "^1.1",
        "ramsey/uuid": "^4.1"
    }
}
